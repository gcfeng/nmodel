{
  "name": "nmodel",
  "version": "1.1.0",
  "description": "Lightweight wrapper for redux and redux-thunk",
  "main": "lib/nmodel.js",
  "module": "es/nmodel.js",
  "unpkg": "dist/nmodel.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "es",
    "src",
    "dist",
    "types"
  ],
  "scripts": {
    "clean": "rimraf lib dist es types",
    "build": "rollup -c",
    "pretest": "npm run build",
    "prepare": "npm run clean && npm run check-types && npm run lint && npm test",
    "check-types": "tsc --noEmit",
    "lint": "eslint \"{src,test}/**/*.{js,ts}\"",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcfeng/nmodel.git"
  },
  "keywords": [
    "redux",
    "redux-thunk",
    "flux",
    "model",
    "state",
    "redux wrapper",
    "elm",
    "predictable"
  ],
  "author": "gcfeng <yjfengwen@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "invariant": "^2.2.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@types/invariant": "^2.2.31",
    "@types/node": "^12.12.21",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^5.2.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^2.7.1",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.7.3"
  },
  "sideEffects": false
}
